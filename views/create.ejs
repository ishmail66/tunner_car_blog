<body>
    <% include partials/menu %>
        <div class="container">
        
        <div class="panel panel-danger">
            <div class="panel panel-heading text-center">
                <h1>Share Your Modifications and car specs Here</h1>
            </div>    
                           
                <form method="POST" action="">
                    <div class="form-group col-xs-12 col-sm-6 col-sm-offset-3">
                        <label for="exampleInputName">Name of POST</label>
                        <input type="text" class="form-control click2edit" id="exampleInputName" aria-describedby="heroName" placeholder="Car Description" name="name">
                         <!-- <small id=heroName" class="form-text text-muted">We'll never share your email with anyone else.</small>  -->
                    </div>
                    <div class="form-group col-xs-12 col-sm-6 col-sm-offset-3">
                            <label for="exampleInputDescription">description</label>
                            <input type="text" class="form-control click2edit" id="exampleInputDescription" placeholder="Link to description" name="description">
                        </div>
                    <div class="form-group col-xs-12 col-sm-6 col-sm-offset-3">
                        <label for="exampleInputURL">Image URL</label>
                        <input type="text" class="form-control click2edit" id="exampleInputURL" placeholder="Link to car" name="image_url">
                    </div> 
                     <!-- <div class="form-group col-xs-12 col-sm-6 col-sm-offset-3">
                            <label for="exampleInputDescription">description</label>
                            <input type="text" class="form-control click2edit" id="exampleInputDescription" placeholder="Link to description" name="description">
                        </div> -->
                    <div class="col-xs-12 col-sm-6 col-sm-offset-3">
                        <button type="submit" class="btn btn-success btn-block">ADD a POST</button>
                    </div>
                </form>

            </div>
            
            
        </div> 
        <button id="edit" class="btn btn-primary" onclick="edit()" type="button">Edit 1</button>
        <button id="save" class="btn btn-primary" onclick="save()" type="button">Save 2</button>

    <script>
            $(document).ready(function() {
             $('#summernote').summernote();
                });
            var edit = function() {
                $('.click2edit').summernote({focus: true});
                };
                var save = function() {
                var markup = $('.click2edit').summernote('code');
                $('.click2edit').summernote('destroy');
                };
    </script>

    
